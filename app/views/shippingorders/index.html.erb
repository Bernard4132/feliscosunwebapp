<style type="text/css">
  a {
    color: blue;
  }

  a:hover {
    color: blue;
  }
</style>


<div class="container">
  <div class="row">
    <a href="/admindashboard" class="btn btn-dark printbtn" style="margin-top: 30px;">Go Back</a>
  </div>
  <br>
  <div class="row">
    <div class="col-lg-12">

        <h4>Search Results</h4>
        
        <div class="table-responsive">
          <table class="table">
            <thead>
              <tr>
                <th>From</th>
                <th>Container no.</th>
                
                
                <th colspan="3"></th>
              </tr>
            </thead>

            <tbody>
              <% @shippingorders.each do |shippingorder| %>
                <tr>
                  <td><%= shippingorder.fromcomp %></td>
                  <td><%= shippingorder.containerno %></td>
                  
                  
                  <td><%= link_to 'Show', shippingorder %></td>
                  <td><%= link_to 'Edit', edit_shippingorder_path(shippingorder) %></td>
                  <td><%= link_to 'Destroy', shippingorder, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                </tr>
              <% end %>
            </tbody>
        </table>
        </div>

        <br>
    </div>
  </div>
</div>